{
  "settings": {
    "root": {
      "pathToSassDirectory": "web/static",
      "sassDirectory": "css",
      "sassImportFile": "app.scss"
    },
    "modules": {
      "modulesDirectory": "modules",
      "modulesImportFile": "_modules.scss",
      "moduleFiles": [
        "_shell.scss",
        "_sidenav.scss"
      ]
    }
  },
  "template": {
    "directories": [
      "global",
      "libs"
    ],
    "files": [
      "app.scss",
      "global/_base.scss",
      "global/_layout.scss",
      "global/_settings.scss",
      "global/_skin.scss",
      "global/_typography.scss",
      "global/_utilities.scss",
      "libs/_library-variable-overrides.scss"
    ]
  }
}